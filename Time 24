#include<stdio.h>
int main()
 {
int hours=1;
int minutes=15;
int seconds=10;
minutes+=seconds/60;
seconds=seconds%60;
hours+=minutes/60;
minutes=minutes%60;
printf("total number of hours is%d\n",hours);
printf("total number of minutes is%d\n",minutes);
printf("total number of seconds is%d\n",seconds);
return 0;
 }

